{
  "name": "auth-m",
  "version": "1.0.0",
  "description": "Auth server made with node, express, mysql & sequelize",
  "main": "index.js",
  "scripts": {
    "start": "bash scripts/production.script.sh",
    "dev": "bash scripts/development.script.sh",
    "gm": "bash scripts/generate-migration.script.sh",
    "build": "tsc",
    "test": "cross-env NODE_CONFIG_DIR=./src/config jest --config jest.config.ts",
    "lint": "eslint 'src/**/*.ts'",
    "precommit": "concurrently \"npm run lint\" \"npm run test\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poojyapitaji/auth-m.git"
  },
  "author": "Shubham Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/poojyapitaji/auth-m/issues"
  },
  "homepage": "https://github.com/poojyapitaji/auth-m#readme",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/config": "^3.3.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/morgan-json": "^1.1.3",
    "@types/node-uuid": "^0.0.32",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "concurrently": "^8.2.2",
    "config": "^3.3.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "mysql2": "^3.9.7",
    "node-uuid": "^1.4.8",
    "sequelize": "^6.37.3"
  }
}
